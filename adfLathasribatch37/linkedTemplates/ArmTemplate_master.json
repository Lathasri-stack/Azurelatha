{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfLathasribatch37"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"ls_azureblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_azureblob'"
		},
		"ls_localfilesystem_onprem_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_localfilesystem_onprem'"
		},
		"ls_onprem_oracle_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_onprem_oracle'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://batch37adlsgen2.dfs.core.windows.net/"
		},
		"ls_AzureKeyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://storepass.vault.azure.net/"
		},
		"ls_localfilesystem_onprem_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\files"
		},
		"ls_localfilesystem_onprem_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "Latha"
		},
		"trigger1_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/dc159867-cc17-421a-8fcc-a0a5116e52be/resourceGroups/Training/providers/Microsoft.Storage/storageAccounts/batch37adlsgen2"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfLathasribatch37_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"ls_azureblob_connectionString": {
						"value": "[parameters('ls_azureblob_connectionString')]"
					},
					"ls_localfilesystem_onprem_password": {
						"value": "[parameters('ls_localfilesystem_onprem_password')]"
					},
					"ls_onprem_oracle_connectionString": {
						"value": "[parameters('ls_onprem_oracle_connectionString')]"
					},
					"ls_AzureKeyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_AzureKeyvault_properties_typeProperties_baseUrl')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"ls_localfilesystem_onprem_properties_typeProperties_host": {
						"value": "[parameters('ls_localfilesystem_onprem_properties_typeProperties_host')]"
					},
					"ls_localfilesystem_onprem_properties_typeProperties_userId": {
						"value": "[parameters('ls_localfilesystem_onprem_properties_typeProperties_userId')]"
					},
					"trigger1_properties_typeProperties_scope": {
						"value": "[parameters('trigger1_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}